{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "singularName": "home-page",
    "pluralName": "home-pages",
    "displayName": "home-page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sliders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::slider.slider"
    },
    "aboutUs": {
      "type": "richtext"
    },
    "topics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::topic.topic"
    },
    "Links": {
      "type": "component",
      "repeatable": true,
      "component": "component.links"
    },
    "slideShowImages": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "internalLinks": {
      "type": "component",
      "repeatable": true,
      "component": "component.internal-links"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    }
  }
}
